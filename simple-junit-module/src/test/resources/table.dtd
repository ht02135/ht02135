<!ELEMENT dataset (
    ANIMAL*,
    AUCTION_BID*,
    AUCTION_ITEM*,
    AUCTION_USER*,
    CLIENT_CACHE*,
    DOCUMENT*,
    DOCUMENT_INDEX_REQUEST*,
    DOMAIN*,
    DOMAIN_SETTING*,
    DOMAIN_USER*,
    IMPLICIT_POLYMORPHIC_CAT*,
    IMPLICIT_POLYMORPHIC_DOG*,
    INDEX_REQUEST*,
    JOINED_ANIMAL*,
    JOINED_CAT*,
    JOINED_DOG*,
