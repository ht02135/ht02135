<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>CUSTOM-Debug jetty project</actionName>
            <displayName>Debug jetty project</displayName>
            <preAction>build-with-dependencies</preAction>
            <goals>
                <goal>jetty:run</goal>
            </goals>
            <properties>
                <skipTests>true</skipTests>
                <maven.test.skip>true</maven.test.skip>
                <jpda.listen>maven</jpda.listen>
                
                <netbeans.deploy.debugmode>true</netbeans.deploy.debugmode>
                <netbeans.deploy>true</netbeans.deploy>
                
                
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-Run jetty project</actionName>
            <displayName>Run jetty project</displayName>
            <preAction>build-with-dependencies</preAction>
            <goals>
                <goal>jetty:run</goal>
            </goals>
            <properties>
                <skipTests>true</skipTests>
                <maven.test.skip>true</maven.test.skip>
                
                
                
                
                <netbeans.deploy>true</netbeans.deploy>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <goals>
                <goal>package</goal>
            </goals>
            <properties>
                <skipTests>true</skipTests>
                <maven.test.skip>true</maven.test.skip>
                
                <netbeans.deploy.profilemode>true</netbeans.deploy.profilemode>
                
                <netbeans.deploy>true</netbeans.deploy>
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-Stop jetty project</actionName>
            <displayName>Stop jetty project</displayName>
            <goals>
                <goal>jetty:stop</goal>
            </goals>
        </action>
        
        
        
        
        
        
        
        
    </actions>
