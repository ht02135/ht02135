    SINGLE_TABLE_ANIMAL*,
    SUPERANIMAL2*,
    SUPERCAT*,
    SUPERCAT2*,
    SUPERDOG*,
    SUPERDOG2*,
    TABLE_PER_CLASS_CAT*,
    TABLE_PER_CLASS_DOG*,
    TERM*,
    TERM_DOCUMENT_INDEX*,
    TERM_INDEX_REQUEST*)>

<!ELEMENT ANIMAL EMPTY>
<!ATTLIST ANIMAL
